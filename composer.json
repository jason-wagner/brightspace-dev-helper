{
	"name": "jason-wagner/brightspace-dev-helper",
	"type": "library",
	"description": "This package allows for easier usage of D2L's Data Hub and Valence tools for their Brightspace LMS.",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Jason Wagner",
			"email": "jason@jasonwagner.dev",
			"homepage": "https://jasonwagner.dev"
		}
	],
	"require": {
		"php": ">=8.1",
		"guzzlehttp/guzzle": "^7.2",
		"vlucas/phpdotenv": "^5.3",
		"nesbot/carbon": "^2.48",
		"ext-json": "*",
		"illuminate/database": "^8.83",
		"league/csv": "^9.8"
	},
	"autoload": {
		"classmap": ["client-custom/"],
		"psr-4": {
			"BrightspaceDevHelper\\Valence\\Client\\": "src/Clients",
			"BrightspaceDevHelper\\Valence\\Block\\": "src/Blocks",
			"BrightspaceDevHelper\\Valence\\CreateBlock\\": "src/CreateBlocks",
			"BrightspaceDevHelper\\Valence\\UpdateBlock\\": "src/UpdateBlocks",
			"BrightspaceDevHelper\\Valence\\PatchBlock\\": "src/PatchBlocks",
			"BrightspaceDevHelper\\Valence\\BlockArray\\": "src/BlockArrays",
			"BrightspaceDevHelper\\Valence\\Structure\\": "src/Structures",
			"BrightspaceDevHelper\\DataHub\\Model\\": "src/Models",
			"BrightspaceDevHelper\\Valence\\SDK\\": "src/ValenceSDK",
			"BrightspaceDevHelper\\Valence\\Attributes\\": "src/Attributes",
			"BrightspaceDevHelper\\Valence\\Object\\": "src/Objects",
			"BrightspaceDevHelper\\Valence\\Array\\": "src/Arrays"
		}
	}
}
